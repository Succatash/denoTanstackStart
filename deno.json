    "react": "npm:react@^19.0.0",
    "react-day-picker": "npm:react-day-picker@^9.5.1",
    "react-dom": "npm:react-dom@^19.0.0",
    "react-hook-form": "npm:react-hook-form@^7.54.2",
    "react-resizable-panels": "npm:react-resizable-panels@^2.1.7",
    "recharts": "npm:recharts@^2.15.1",
    "redaxios": "npm:redaxios@^0.5.1",
    "sonner": "npm:sonner@^2.0.1",
    "tailwind-merge": "npm:tailwind-merge@^3.0.2",
    "tailwindcss": "npm:tailwindcss@^4.0.9",
    "typescript": "npm:typescript@^5.8.2",
    "vaul": "npm:vaul@^1.1.2",
    "vinxi": "npm:vinxi@^0.5.3",
    "zod": "npm:zod@^3.24.2"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxFactory": "React.createElement",
    "jsxFragmentFactory": "React.Fragment",

    "lib": ["DOM", "DOM.Iterable", "ESNext", "Deno.Unstable", "Deno.ns", ""],
    "strictNullChecks": true,
    "allowJs": true,
    "strict": true,
    "types": ["node"]
  },
  "exclude": ["node_modules", "dist"]
}
