    "@radix-ui/react-separator": "npm:@radix-ui/react-separator@^1.1.2",
    "@radix-ui/react-slider": "npm:@radix-ui/react-slider@^1.2.3",
    "@radix-ui/react-slot": "npm:@radix-ui/react-slot@^1.1.2",
    "@radix-ui/react-switch": "npm:@radix-ui/react-switch@^1.1.3",
    "@radix-ui/react-tabs": "npm:@radix-ui/react-tabs@^1.1.3",
    "@radix-ui/react-toggle": "npm:@radix-ui/react-toggle@^1.1.2",
    "@radix-ui/react-toggle-group": "npm:@radix-ui/react-toggle-group@^1.1.2",
    "@radix-ui/react-tooltip": "npm:@radix-ui/react-tooltip@^1.1.8",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.0.9",
