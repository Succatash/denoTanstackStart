{
  "type": "module",
  "tasks": {
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "vinxi start",
    "db:generate": "deno -A  --unstable-detect-cjs npm:drizzle-kit generate",
    "db:push": "deno run --allow-net --node-modules-dir --allow-env --allow-read --allow-sys --allow-write --allow-run npm:drizzle-kit push",
    "serve": "deno serve --watch --allow-net --allow-env --allow-read --allow-sys --env-file=.env.development ./app/ssr.tsx"
  },
  "imports": {
    "@/": "./app/",
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.4",
    "@hookform/resolvers": "npm:@hookform/resolvers@^4.1.3",
    "@neondatabase/serverless": "npm:@neondatabase/serverless@^0.10.4",
    "@radix-ui/react-accordion": "npm:@radix-ui/react-accordion@^1.2.3",
    "@radix-ui/react-alert-dialog": "npm:@radix-ui/react-alert-dialog@^1.1.6",
    "@radix-ui/react-aspect-ratio": "npm:@radix-ui/react-aspect-ratio@^1.1.2",
    "@radix-ui/react-avatar": "npm:@radix-ui/react-avatar@^1.1.3",
    "@radix-ui/react-checkbox": "npm:@radix-ui/react-checkbox@^1.1.4",
    "@radix-ui/react-collapsible": "npm:@radix-ui/react-collapsible@^1.1.3",
    "@radix-ui/react-context-menu": "npm:@radix-ui/react-context-menu@^2.2.6",
    "@radix-ui/react-dialog": "npm:@radix-ui/react-dialog@^1.1.6",
    "@radix-ui/react-dropdown-menu": "npm:@radix-ui/react-dropdown-menu@^2.1.6",
    "@radix-ui/react-hover-card": "npm:@radix-ui/react-hover-card@^1.1.6",
    "@radix-ui/react-label": "npm:@radix-ui/react-label@^2.1.2",
    "@radix-ui/react-menubar": "npm:@radix-ui/react-menubar@^1.1.6",
    "@radix-ui/react-navigation-menu": "npm:@radix-ui/react-navigation-menu@^1.2.5",
    "@radix-ui/react-popover": "npm:@radix-ui/react-popover@^1.1.6",
    "@radix-ui/react-progress": "npm:@radix-ui/react-progress@^1.1.2",
    "@radix-ui/react-radio-group": "npm:@radix-ui/react-radio-group@^1.2.3",
