    "@types/react": "npm:@types/react@^19.0.10",
    "@types/react-dom": "npm:@types/react-dom@^19.0.4",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.4",
    "class-variance-authority": "npm:class-variance-authority@^0.7.1",
    "clsx": "npm:clsx@^2.1.1",
    "cmdk": "npm:cmdk@^1.0.4",
    "data-fns": "npm:data-fns@^1.1.0",
    "drizzle-kit": "npm:drizzle-kit@^0.30.5",
    "drizzle-orm": "npm:drizzle-orm@^0.40.0",
    "drizzle-zod": "npm:drizzle-zod@^0.7.0",
    "embla-carousel-react": "npm:embla-carousel-react@^8.5.2",
    "input-otp": "npm:input-otp@^1.4.2",
    "lucide-react": "npm:lucide-react@^0.477.0",
